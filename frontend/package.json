    "@mui/lab": "^6.0.0-beta.27",
    "@mui/material": "^6.4.4",
    "@mui/x-charts": "^8.4.0",
    "@mui/x-date-pickers": "^7.26.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
